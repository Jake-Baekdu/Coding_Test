   "source": [
    "N, K = map(int, input().split())\n",
    "coin = [0]*N\n",
    "for i in range(N):\n",
    "    coin[i] = int(input())\n",
    "\n",
    "cnt = 0\n",
    "for i in coin[::-1]:\n",
    "    if K >= i:\n",
    "        temp = K//i\n",
    "        K -= i*temp\n",
    "        cnt += temp\n",
    "\n",
    "print(cnt)\n",
    "    "
   ]
  }
