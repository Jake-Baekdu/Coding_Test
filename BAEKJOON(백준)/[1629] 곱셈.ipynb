    "from sys import stdin\n",
    "A, B, C = map(int, stdin.readline().split())\n",
    "\n",
    "def cal(A,B):\n",
    "\n",
    "    if B == 1 :\n",
    "        return A%C\n",
    "    temp = cal(A, B//2)\n",
    "    if B%2 == 0:\n",
    "        return  (temp * temp)%C\n",
    "    elif B%2 == 1:\n",
    "        return (temp * cal(A, B//2 + 1))%C\n",
    "    \n",
    "print(cal(A,B))"
   ]
  }
