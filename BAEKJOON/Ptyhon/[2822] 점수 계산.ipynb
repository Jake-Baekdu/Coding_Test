    "\n",
    "arr = [0 for _ in range(8)]\n",
    "\n",
    "for i in range(8):\n",
    "    arr[i] = [int(input()), i+1]\n",
    "    \n",
    "arr.sort()\n",
    "SUM = 0\n",
    "ans_nums = set()\n",
    "for i in range(3,8):\n",
    "    SUM += arr[i][0]\n",
    "    ans_nums.add(arr[i][1])\n",
    "\n",
    "print(SUM)\n",
    "print(*ans_nums)\n",
    "    \n",
    "\n",
    "    \n"
   ]
  }
