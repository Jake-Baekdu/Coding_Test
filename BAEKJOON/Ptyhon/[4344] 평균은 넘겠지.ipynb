   "source": [
    "n = int(input())\n",
    "\n",
    "for _ in range(n):\n",
    "    arr = list(map(int, input().split()))\n",
    "    s = arr[0]\n",
    "    arr = arr[1:]\n",
    "    avg = sum(arr)/s\n",
    "    cnt = 0\n",
    "    for i in arr:\n",
    "        if i > avg:\n",
    "            cnt += 1\n",
    "            \n",
    "    result = cnt/s*100\n",
    "    print('{:.3f}%'.format(result))"
   ]
  }
