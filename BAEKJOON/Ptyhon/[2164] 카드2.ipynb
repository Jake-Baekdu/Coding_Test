   "source": [
    "from sys import stdin\n",
    "from collections import deque\n",
    "\n",
    "n = int(input())\n",
    "if n == 1:\n",
    "    print(1)\n",
    "    exit()\n",
    "dq = deque([i for i in range(1, n+1)])\n",
    "\n",
    "while(True):\n",
    "    dq.popleft()\n",
    "    if len(dq) == 1:\n",
    "        break\n",
    "    tmp = dq.popleft()\n",
    "    dq.append(tmp)\n",
    "print(dq[0])"
   ]
  }
