   "source": [
    "n = int(input())\n",
    "\n",
    "students = [[] for _ in range(n)]\n",
    "\n",
    "for i in range(n):\n",
    "    tmp = list(input().split(\" \"))\n",
    "    students[i] = [tmp[0], int(tmp[1]), int(tmp[2]), int(tmp[3])]\n",
    "\n",
    "\n",
    "students.sort(key = lambda x : (-x[1], x[2], -x[3], x[0]))\n",
    "\n",
    "\n",
    "for i in range(n):\n",
    "    print(students[i][0])"
   ]
  }
