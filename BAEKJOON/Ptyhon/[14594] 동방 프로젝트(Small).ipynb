    "N = int(input())\n",
    "M = int(input())\n",
    "rooms = [0] * (N+1)\n",
    "for _ in range(M):\n",
    "    x, y = map(int, input().split())\n",
    "    for room in range(x, y):\n",
    "        rooms[room] = 1\n",
    "    print(rooms)\n",
    "answer = rooms.count(0)\n",
    "print(answer-1)"
   ]
