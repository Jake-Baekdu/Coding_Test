   "source": [
    "n = int(input())\n",
    "dp = [list(map(int, input().split())) for _ in range(n)]\n",
    "for i in range(1, n):\n",
    "    dp[i][0] = dp[i][0] + min(dp[i-1][1], dp[i-1][2])\n",
    "    dp[i][1] = dp[i][1] + min(dp[i-1][0], dp[i-1][2])\n",
    "    dp[i][2] = dp[i][2] + min(dp[i-1][1], dp[i-1][0])\n",
    "\n",
    "print(min(dp[n-1]))"
   ]
