   "source": [
    "import itertools\n",
    "\n",
    "N, M = map(int, input().split())\n",
    "\n",
    "cards = list(map(int, input().split(\" \")))\n",
    "ans = 0\n",
    "combi_cards = list(map(tuple, itertools.combinations(cards, 3)))\n",
    "for c in combi_cards:\n",
    "    sum_c = sum(c)\n",
    "    if sum_c <= M and sum_c > ans:\n",
    "        ans = sum_c\n",
    "print(ans)"
   ]
