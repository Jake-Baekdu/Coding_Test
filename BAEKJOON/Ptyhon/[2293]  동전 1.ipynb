   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "coin = [0]*n\n",
    "dp = [0]*(k+1) # dp 범위 k+1까지\n",
    "for i in range(n):\n",
    "    coin[i] = int(input())\n",
    "coin"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [
    {
     "data": {
