    "n = int(input())\n",
    "\n",
    "temp = 1\n",
    "answer = 0\n",
    "\n",
    "while True:\n",
    "    n -= temp\n",
    "    \n",
    "    if n >= 0:\n",
    "        answer += 1\n",
    "        temp += 1\n",
    "        \n",
    "    else:\n",
    "        print(answer)\n",
    "        break"
   ]
  }
